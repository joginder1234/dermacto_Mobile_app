{
  "expo": {
    "name": "Medicare_App",
    "slug": "Medicare_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/launcher.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
           "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/launcher.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "medicare.apk"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
